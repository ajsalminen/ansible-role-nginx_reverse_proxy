{% for proxy_cache_option in nginx_reverse_proxy_cache_options %}
    {{ proxy_cache_option }}
{% endfor %}
    proxy_cache STATIC;
    proxy_cache_key "$scheme$host$request_uri";
    proxy_cache_valid  200 302 301 {{ proxy_cache_valid_time }};
    proxy_cache_valid  404      1m;
    proxy_cache_lock on;
    proxy_cache_lock_timeout 60000;
